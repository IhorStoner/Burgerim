rder)\n  const errors = useSelector(state => state.form.formOrder.syncErrors)\n  const [nameValid, setNameValid] = useState(false);\n  const [phoneValid, setPhoneValid] = useState(false);\n  const [formValid, setFormValid] = useState(false)\n\n  useEffect(() => {\n    if (errors) {\n      if (!errors.name) {\n        setNameValid(true)\n      } else {\n        setNameValid(false)\n      }\n      if (!errors.phone) {\n        setPhoneValid(true)\n      } else {\n        setPhoneValid(false)\n      }\n    } else {\n      setPhoneValid(true)\n      setNameValid(true)\n      setFormValid(true)\n    }\n  }, [formData])\n\n\n\n\n\n\n  return (\n    <div>\n      {\n        isOpenPopup &&\n        <div className='formOrder' >\n          <form className='formOrder__form' onSubmit={handleSubmit}>\n            <button className='formOrder__btnClose' onClick={() => dispatch(openPopup())}></button>\n            <div className=\"formOrder__content\">\n              <div className='formOrder__row'>\n                <div className='formOrder__text'>Имя:</div>\n                <Field className={!nameValid ? 'formOrder__input--red' : 'formOrder__input'} name='name' component={TextField} placeholder='Имя'></Field>\n              </div>\n              <div className='formOrder__row'>\n                <div className='formOrder__text'>Телефон:</div>\n                <Field className={!phoneValid ? 'formOrder__input--red' : 'formOrder__input'} name='phone' component={TextField} placeholder='Телефон'></Field>\n              </div>\n              <button className='formOrder__btnSubmit' type='submit' disabled={!valid && !submitting}>Отправить</button>\n            </div>\n          </form>\n        </div>\n      }\n    </div>\n  )\n}\n\nconst validate = values => {\n  const errors = {};\n\n  const regName = /^[a-zA-Zа-яА-ЯёЁ'][a-zA-Z-а-яА-ЯёЁ' ]+[a-zA-Zа-яА-ЯёЁ']?$/\n  if (!regName.test(values.name)) {\n    errors.name = 'Не правильное имя'\n  }\n  if (!values.name) {\n    errors.name = 'Обязательно'\n  }\n  const regPhone = /^((\\+)?(3)?(8)?[\\- ]?)?(\\(?\\d{3}\\)?[\\- ]?)?\\d{3}[\\- ]?\\d{2}[\\- ]?\\d{2}$/\n  if (!regPhone.test(values.phone)) {\n    errors.phone = 'Не правильный телефон'\n  }\n  if (!values.phone) {\n    errors.phone = 'Обязательно'\n  }\n  return errors\n}\n\n\nexport default reduxForm({\n  form: \"formOrder\",\n  validate\n})(FormOrder);","C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\redux\\rootReducer.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\redux\\actions\\popUpAction.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\TextField\\TextField.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\redux\\selectors\\popUpSelector.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\redux\\reducers\\popUpReducer.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\MenuList\\MenuList.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\Burgers\\Burgers.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\Garnish\\Garnish.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\Title\\Title.js",["109"],"import React from 'react'\r\nimport './Title.scss'\r\n\r\nexport default function Title({ text, name, className }) {\r\n  return (\r\n    <div className=\"titleContainer\">\r\n      <a name={name}><h1 className={`title ${className}`}>{text}</h1></a>\r\n    </div>\r\n  )\r\n}\r\n","C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\Drinks\\Drinks.js",[],"C:\\Users\\admin\\Desktop\\Burgermain\\Burgers-master\\client\\src\\components\\MeatInfo\\MeatInfo.js",[],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":4,"column":1,"nodeType":"116","endLine":9,"endColumn":3},{"rul